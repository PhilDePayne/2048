 207:main.c        ****     if (anyKey != KEY_NOTHING)
 208:main.c        ****     {
 209:main.c        ****       //select specific function
 210:main.c        ****       if (anyKey == KEY_CENTER)
 211:main.c        ****       {
 212:main.c        ****         switch(cursor)
 213:main.c        ****         {
 214:main.c        ****           case 0: startGameAsClient(); break;
 215:main.c        ****           case 1: play2048(0); break;
 216:main.c        **** 
 217:main.c        ****           case 2: startGameAsServer(); break;
 218:main.c        **** 
 219:main.c        ****           case 3: handleBt(); break;
 220:main.c        ****           case 4: displayScores(); break;
 221:main.c        ****           case 5: clearScores(); break;
 222:main.c        ****           default: break;
 223:main.c        ****         }
 224:main.c        ****         drawMenu();
 225:main.c        ****       }
 226:main.c        ****       
 227:main.c        ****       //move cursor up
 228:main.c        ****       else if (anyKey == KEY_UP)
 229:main.c        ****       {
 230:main.c        ****         if (cursor > 0)
 231:main.c        ****           cursor--;
 232:main.c        ****         else
 233:main.c        ****           cursor = 5;
 234:main.c        ****         drawMenuCursor(cursor);
 235:main.c        ****       }
 236:main.c        ****       
 237:main.c        ****       //move cursor down
 238:main.c        ****       else if (anyKey == KEY_DOWN)
 239:main.c        ****       {
 240:main.c        ****         if (cursor < 5)
 241:main.c        ****           cursor++;
 242:main.c        ****         else
 243:main.c        ****           cursor = 0;
 244:main.c        ****         drawMenuCursor(cursor);
 245:main.c        ****       }
 246:main.c        ****       
 247:main.c        ****       //adjust contrast
 248:main.c        ****       else if (anyKey == KEY_RIGHT)
 249:main.c        ****       {
 250:main.c        ****         contrast++;
