 252:main.c        ****           contrast = 127;
 253:main.c        ****         lcdContrast(contrast);
 254:main.c        ****       }
 255:main.c        ****       else if (anyKey == KEY_LEFT)
 256:main.c        ****       {
 257:main.c        ****         if (contrast > 0)
 258:main.c        ****           contrast--;
 259:main.c        ****         lcdContrast(contrast);
 260:main.c        ****       }
 261:main.c        ****     }
