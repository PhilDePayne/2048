 185:main.c        ****       setLED(LED_GREEN, TRUE);
 186:main.c        ****       setLED(LED_RED,   FALSE);
 187:main.c        ****       osSleep(1);
 188:main.c        ****     }
 189:main.c        ****     osSleep(10);
